{
    "eslint.enable": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true
    },
    "cSpell.words": [
        "isbot",
        "Pipeable",
        "tailwindcss",
        "prismjs",
        "headlessui",
        "typecheck",
        "graphcms",
        "Quickstart",
        "Hygraph",
        "CONVERTKIT",
        "twitterx"
    ]
}
